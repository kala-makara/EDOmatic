{
  "name": "SvelteKit Extension",
  "description": "A browser extension made with Svelte Kit",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "options_page": "options.html",
  "action": {
    "default_title": "Svelte Extension",
    "default_icon": {
        "16": "favicon.png",
        "24": "favicon.png",
        "32": "favicon.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "48": "favicon.png"
  }
}